<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.test.mapper.CollegeMapper">
    <select id="getCollegeCount" resultType="int">
        SELECT COUNT(*) FROM `cil_management`.t_college;
    </select>

    <select id="getOneCollege" resultType="com.test.po.College">
        SELECT id as i, name as n FROM t_class LIMIT 1
    </select>
</mapper>